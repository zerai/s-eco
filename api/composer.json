{
    "name": "medicalmundi/oe-module-todo-list",
    "type": "project",
    "description": "Symfony ecosystem.",
    "keywords": [
        "symfony",
        "symfony bundles",
        "symfony projects"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Teclai Zerai",
            "email": "teclaizerai@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": "^3.4",
        "api-platform/symfony": "^3.4",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "ecotone/amqp": "^1.236",
        "ecotone/dbal": "^1.236",
        "ecotone/jms-converter": "^1.236",
        "ecotone/symfony-bundle": "^1.236",
        "luzrain/workerman-bundle": "^0.3.0",
        "nelexa/roach-php-bundle": "^1.1",
        "nelmio/cors-bundle": "^2.5",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^1.33",
        "symfony/asset": "6.4.*",
        "symfony/cache": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "6.4.*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.61",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "audit": {
            "abandoned": "report"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ingesting\\": "context/ingesting/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "IngestingTests\\": "context/ingesting/tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "ark": "./vendor/bin/phparkitect check --ansi",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "cs": "./vendor/bin/ecs",
        "cs:fix": "./vendor/bin/ecs --fix"
    },
    "scripts-descriptions": {
        "ark": "Checks architecture rules",
        "cs": "Check php coding standard",
        "cs:fix": "Fix php coding standard"
    }
}
